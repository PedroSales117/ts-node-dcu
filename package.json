{
  "name": "ts-template-api",
  "version": "1.0.0",
  "description": "---",
  "main": "dist/app.js",
  "scripts": {
    "prepare": "husky",
    "start:dev": "esrun src/app.ts --target=node18 --sourcemap --splitting --outdir=dist --platform=node --packages=external --tsconfig=./tsconfig.json",
    "start": "node dist/app.js",
    "build": "esbuild src/app.ts --target=node18 --bundle --outdir=dist --platform=node --packages=external --tsconfig=./tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.config.js",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PedroSales117/ts-template-api.git"
  },
  "keywords": [
    "template",
    "api",
    "typescript",
    "service",
    "neverthrow"
  ],
  "author": "PedroSales117",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PedroSales117/ts-template-api/issues"
  },
  "homepage": "https://github.com/PedroSales117/ts-template-api#readme",
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@digitak/esrun": "3.2.26",
    "@fastify/jwt": "^9.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-neverthrow": "1.1.4",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "5.4.2"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^10.0.1",
    "@fastify/sensible": "^6.0.1",
    "@nestjs/class-validator": "^0.13.4",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "fastify": "^5.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "neverthrow": "6.1.0",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.1",
    "pino": "^9.3.2",
    "pino-pretty": "^11.2.2",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  }
}
