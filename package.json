{
  "name": "ts-template-api",
  "version": "1.0.0",
  "description": "Template for API service in typescript.",
  "main": "dist/app.js",
  "scripts": {
    "prepare": "husky",
    "start:dev": "esrun src/app.ts --target=node18 --sourcemap --splitting --outdir=dist --platform=node --packages=external --tsconfig=./tsconfig.json",
    "start": "node dist/app.js",
    "build": "esbuild src/app.ts --target=node18 --bundle --outdir=dist --platform=node --packages=external --tsconfig=./tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/PedroSales117/ts-template-api.git"
  },
  "keywords": [
    "template",
    "api",
    "typescript",
    "service",
    "neverthrow"
  ],
  "author": "PedroSales117",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PedroSales117/ts-template-api/issues"
  },
  "homepage": "https://github.com/PedroSales117/ts-template-api#readme",
  "devDependencies": {
    "@commitlint/cli": "19.2.1",
    "@commitlint/config-conventional": "19.1.0",
    "@digitak/esrun": "3.2.26",
    "@types/node": "20.11.30",
    "@typescript-eslint/eslint-plugin": "7.3.1",
    "@typescript-eslint/parser": "7.3.1",
    "esbuild": "0.20.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-neverthrow": "1.1.4",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "^9.0.11",
    "typescript": "5.4.2"
  },
  "dependencies": {
    "fastify": "4.26.2",
    "http-status-codes": "^2.3.0",
    "neverthrow": "6.1.0"
  }
}
